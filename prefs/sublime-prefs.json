{
	"bold_folder_labels": true,
	"default_encoding": "UTF-8",
	"default_line_ending": "system",
	"draw_white_space": "all",
	"ensure_newline_at_eof_on_save": true,
	"file_exclude_patterns":
	[
		"*.a",
		"*.class",
		"*.db",
		"*.dll",
		"*.DS_Store",
		"*.dylib",
		"*.exe",
		"*.idb",
		"*.lib",
		"*.ncb",
		"*.o",
		"*.obj",
		"*.pdb",
		"*.psd",
		"*.pyc",
		"*.pyo",
		"*.sdf",
		"*.so",
		"*.sublime-workspace",
		"*.suo",
		"._*",
		".directory",
		".DS_Store",
		".Spotlight-V100",
		"desktop.ini"
	],
	"folder_exclude_patterns":
	[
		"bazel-*",
		"CVS",
		".git",
		".hg",
		".svn",
		".Trash",
		".Trash-*"
	],
	"font_face": "MonoLisa, SF Mono, Menlo",
	"font_size": 12,
	"hardware_acceleration": "opengl",
	"highlight_line": true,
	"ignored_packages":
	[
		"Vintage"
	],
	"index_exclude_patterns":
	[
		"*.log",
		"*/bazel-*"
	],
	"index_files": true,
	"line_numbers": true,
	"line_padding_bottom": 1,
	"show_encoding": true,
	"show_line_endings": true,
	"tab_size": 2,
	"translate_tabs_to_spaces": true,
	"trim_trailing_white_space_on_save": true,
	"update_check": true,
	"use_tab_stops": true
}
